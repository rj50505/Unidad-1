        boolean esMayorDeEdad = edad >= 18;  

        System.out.println("Edad: " + edad);
        System.out.println("Altura: " + altura + " metros");
        System.out.println("Inicial del nombre: " + inicialNombre);
        System.out.println("Â¿Es mayor de edad? " + esMayorDeEdad);
    }    
}
